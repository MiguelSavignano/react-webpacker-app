{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    "flow",
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-runtime",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [["env"], "react"],
      "plugins": [
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "transform-runtime",
        [
          "transform-class-properties",
          {
            "spec": true
          }
        ]
      ]
    }
  }
}
